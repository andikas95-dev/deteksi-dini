generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

  // id       String @id @default(cuid())
model Users {
  id       Int    @id @default(autoincrement())
  nama     String
  email    String @unique
  password String
  role     String
}
